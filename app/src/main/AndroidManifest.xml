<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.honoriojose.cadastrobukaapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Cadastro"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Detalhes"
            android:label="@string/title_activity_detalhes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FuncionarioLista"
            android:label="@string/title_activity_funcionario_lista"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>